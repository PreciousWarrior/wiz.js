{
  "name": "@preciouswarrior/wiz.js",
  "version": "2.0.5",
  "description": "A js library to interact with the wizemen API",
  "scripts": {
    "prebuild": "rimraf dist && yarn test",
    "build": " rollup -c",
    "test": "jest",
    "lint": "cross-env NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "prettier-eslint \"src/**/*.js\" --write",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PreciousWarrior/wiz.js.git"
  },
  "author": "PreciousWarrior",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PreciousWarrior/wiz.js/issues"
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".babelrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --fix",
      "git add"
    ],
    "*.json,.{babelrc,eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && lint-staged"
    }
  },
  "homepage": "https://github.com/PreciousWarrior/wiz.js#readme",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "prettier-eslint-cli": "^5.0.1",
    "rollup": "^2.56.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
