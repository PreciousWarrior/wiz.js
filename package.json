{
  "name": "@preciouswarrior/wiz.js",
  "version": "2.0.4",
  "description": "A library to interact with the wizemen API",
  "main": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PreciousWarrior/wiz.js.git"
  },
  "author": "PreciousWarrior",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PreciousWarrior/wiz.js/issues"
  },
  "homepage": "https://github.com/PreciousWarrior/wiz.js#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@types/jest": "^27.0.1",
    "babel-jest": "^27.1.0",
    "jest": "^27.0.6",
    "jsdoc": "^3.6.7"
  },
  "scripts": {
    "test": "cd src; jest --watchAll"
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".babelrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{babelrc,eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
